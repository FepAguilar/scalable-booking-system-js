generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Payment {
  id          String   @id @default(uuid())
  bookingId   String
  amount      Float
  currency    String
  status      String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}